{
  "name": "@soybeanjs/vue-materials",
  "version": "0.1.9",
  "description": "vue2 and vue3 components material",
  "author": "Soybean <honghuangdc@gmail.com> (https://github.com/honghuangdc)",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all typecheck vite:build",
    "build:lib": "cross-env VITE_LIB_MODE=1 npm-run-all typecheck vite:build dts",
    "vite:build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "dts": "vue-tsc --declaration --emitDeclarationOnly",
    "test": "npm run test3",
    "test2": "vue-demi-switch 2 vue2",
    "test3": "vue-demi-switch 3"
  },
  "dependencies": {
    "colord": "^2.9.3",
    "vue-demi": "^0.14.1"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.1.3",
    "@unocss/preset-uno": "^0.51.12",
    "@unocss/transformer-compile-class": "^0.51.12",
    "@unocss/vite": "^0.51.12",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1",
    "typed-css-modules": "^0.7.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vue": "^3.3.2",
    "vue-tsc": "^1.6.4",
    "vue2": "npm:vue@3"
  }
}
